@model IEnumerable<SKH_GS_MVC.Models.member>

@{
    ViewBag.Title = "Contact";
}
<h2>聯絡資訊</h2>



<table class="table">
    <tr>
        <th>職稱</th>
        <th>姓名</th>
        <th>簡碼</th>

    </tr>

        @foreach(var contactItem in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => contactItem.title)
                </td>               
                <td>
                    @Html.DisplayFor(modelItem =>contactItem.name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => contactItem.inner_call)
                </td>
            </tr>
        }

</table>
